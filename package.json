{
  "name": "javascript-development-environment",
  "version": "1.1.0",
  "description": "A JS Dev Starter Kit",
  "scripts": {
    "start": "NODE_ENV=development npm-run-all --parallel security-check open:src lint:watch flow test:watch start-mockapi",
    "open:src": "babel-node buildScripts/srcServer.js",
    "security-check": "nsp check",
    "localtunnel": "lt --port 3000",
    "share": "npm-run-all --parallel open:src localtunnel",
    "flow": "./node_modules/.bin/flow",
    "lint": "esw webpack.config.* src buildScripts",
    "lint:watch": "npm run lint -- --watch",
    "test": "jest",
    "test:watch": "npm run test -- --watch",
    "generate-mock-data": "babel-node buildScripts/generateMockData",
    "prestart-mockapi": "npm run generate-mock-data",
    "start-mockapi": "json-server --watch src/api/db.json --port 3001",
    "clean-dist": "rimraf ./dist && mkdir dist",
    "prebuild_________________________________________________________________________________________": "npm-run-all clean-dist test lint",
    "build": "NODE_ENV=production babel-node buildScripts/build.js",
    "deploy": "surge ./dist",
    "rebuild-deps": "npm-run-all --parallel clear-yarn-lock rim-node-modules",
    "clear-yarn-lock": "rimraf yarn.lock",
    "rim-node-modules": "rimraf node_modules"
  },
  "author": "Matt Revell",
  "license": "MIT",
  "dependencies": {
    "react": "^15.5.3",
    "react-dom": "^16.0.1",
    "whatwg-fetch": "1.0.0"
  },
  "devDependencies": {
    "babel-cli": "^6.24.1",
    "babel-core": "^6.24.1",
    "babel-loader": "^6.4.1",
    "babel-preset-killua": "^1.0.5",
    "chalk": "^1.1.3",
    "cheerio": "^0.22.0",
    "compression": "^1.6.2",
    "cross-env": "^4.0.0",
    "css-loader": "^0.28.0",
    "eslint": "^3.19.0",
    "eslint-plugin-killua": "^1.0.1",
    "eslint-watch": "^3.0.1",
    "express": "^4.15.2",
    "extract-text-webpack-plugin": "^2.1.0",
    "flow-bin": "^0.43.1",
    "html-webpack-plugin": "^2.28.0",
    "jest": "^19.0.2",
    "jsdom": "^9.12.0",
    "json-schema-faker": "^0.5.0-rc1",
    "json-server": "^0.9.6",
    "localtunnel": "^1.8.2",
    "nock": "^9.0.13",
    "npm-run-all": "^4.0.2",
    "nsp": "^2.6.3",
    "numeral": "^2.0.6",
    "open": "^0.0.5",
    "prettier": "^0.22.0",
    "rimraf": "^2.6.1",
    "style-loader": "^0.16.1",
    "surge": "^0.19.0",
    "webpack": "^2.3.3",
    "webpack-dev-middleware": "^1.10.1",
    "webpack-hot-middleware": "^2.18.0",
    "webpack-md5-hash": "^0.0.5"
  }
}
